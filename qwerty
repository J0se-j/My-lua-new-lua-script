local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Animation variables
local loadingFrames = {"Loading.", "Loading..", "Loading..."}
local currentFrame = 1
local animationSpeed = 0.5 -- seconds between frames

-- Create an animation loop
local animationThread = coroutine.create(function()
    while true do
        currentFrame = currentFrame + 1
        if currentFrame > #loadingFrames then
            currentFrame = 1
        end
        wait(animationSpeed)
    end
end)
coroutine.resume(animationThread)

-- Function to get the current loading text
local function getLoadingTitle()
    return loadingFrames[currentFrame]
end

local Window = Rayfield:CreateWindow({
   Name = "Who are you?",
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = getLoadingTitle(),
   LoadingSubtitle = "by Jose",
   Theme = "Default", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Who Are You?",
      Subtitle = "Key System",
      Note = "What is your name?", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Maira, Jose"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

local MainTab = Window:CreateTab("ðŸ‘°Who are you? ðŸ§’", nil) -- Title, Image
local Section = MainTab:CreateSection("ðŸ‘°Who are you? ðŸ§’")

local Button = MainTab:CreateButton({
   Name = "Maira",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/J0se-j/My-lua-new-lua-script/main/M"))()
   end,
})

local Button = MainTab:CreateButton({
   Name = "Jose",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/J0se-j/My-lua-new-lua-script/main/J"))()
   end,
})
